{
  "manifest_version": 2,

  "name": "OCR-JPN",
  "description": "Select images with Japanese characters and get a dictionary definition.",
  "version": "1.0",

"permissions": ["tabs", "<all_urls>", "activeTab", "http://127.0.0.1/*"],
"background": {
    "scripts": ["jquery-ui-1.10.3.custom/js/jquery-1.9.1.js","background.js"]
},

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
  ,  "content_scripts": [
    {
      "matches" : ["<all_urls>"],
      "css": ["jquery-ui-1.10.3.custom/css/no-theme/jquery-ui-1.10.3.custom.css", "jquery-ui-1.10.3.custom/css/no-theme/mycss.css"],
      "js": ["jquery-ui-1.10.3.custom/js/jquery-1.9.1.js", "jquery-ui-1.10.3.custom/js/jquery-ui-1.10.3.custom.js", "selectimage.js"]
    }
  ],
  "web_accessible_resources":[
  "jquery-ui-1.10.3.custom/css/no-theme/ui-icons_888888_256x240.png"
  ]
}